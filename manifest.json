{
  "manifest_version": 3,
  "name": "智云课堂视频下载插件",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_icon": "public/icon.svg",
    "default_popup": "public/popup.html"
  },
  "background": {
    "service_worker": "dist/background.js"
  },
  "host_permissions":[
    "https://classroom.zju.edu.cn/*"
  ],
  "permissions": [
    "tabs", 
    "scripting",
    "activeTab",
    "storage",
    "webRequest"
  ]
}